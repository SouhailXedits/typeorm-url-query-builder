  "version": "1.0.6",
  "description": "Url query parser for typeorm",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint --fix -p tsconfig.json",
    "semantic-release": "semantic-release",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SouhailXedits/advanced-typeorm-query-builder.git"
  },
  "keywords": [
    "javascript",
    "typeorm",
    "query",
    "parser"
  ],
  "author": "SouhailXedits",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SouhailXedits/advanced-typeorm-query-builder/issues"
  },
  "homepage": "https://github.com/SouhailXedits/advanced-typeorm-query-builder#readme",
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^7.0.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "semantic-release": "^19.0.2",
    "ts-jest": "^29.1.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^5.0.3"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "date-fns": "^2.29.3",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.0"
